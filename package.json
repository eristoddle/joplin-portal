{
  "name": "joplin-portal",
  "version": "1.0.3",
  "description": "Access and import notes from Joplin server within Obsidian",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "node esbuild.config.mjs production",
    "build:release": "node scripts/build-release.mjs",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "version:bump": "node scripts/version-bump.mjs",
    "prepare:release": "node scripts/prepare-release.mjs",
    "changelog": "node scripts/generate-changelog.mjs",
    "release": "node scripts/release.mjs",
    "release:patch": "node scripts/release.mjs patch",
    "release:minor": "node scripts/release.mjs minor",
    "release:major": "node scripts/release.mjs major",
    "release:auto": "node scripts/release.mjs patch --yes",
    "release:legacy": "npm run build:release && npm run prepare:release",
    "release:complete": "node scripts/release-complete.mjs",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "joplin",
    "notes",
    "import",
    "search",
    "preview",
    "productivity",
    "note-taking",
    "integration"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/joplin-portal.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/joplin-portal/issues"
  },
  "homepage": "https://github.com/your-username/joplin-portal#readme",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "4.7.4",
    "vitest": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "jsdom": "^23.0.0"
  }
}